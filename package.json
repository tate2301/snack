{
	"name": "snack",
	"author": {
		"name": "TypescriptDev",
		"email": "chris@typescriptdev.xyz"
	},
	"description": "A simple snack app for your todos",
	"version": "0.1.0",
	"private": true,
	"homepage": ".",
	"main": "build/electron/main.js",
	"scripts": {
		"start": "react-app-rewired start",
		"build": "react-app-rewired build",
		"test": "react-app-rewired test",
		"postinstall": "electron-builder install-app-deps",
		"electron:dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://127.0.0.1:3000 && tsc -p electron -w\" \"wait-on http://127.0.0.1:3000 && tsc -p electron && electron .\"",
		"electron:build": "yarn build && tsc -p electron && electron-builder build -c.extraMetadata.main=build/electron/main.js",
		"eject": "react-app-rewired eject"
	},
	"build": {
		"appId": "xyz.atipamara.snack",
		"productName": "Snack",
		"directories": {
			"buildResources": "public/assets"
		},
		"win": {
			"icon": "build/icon.png"
		},
		"mac": {
			"category": "xyz.atipamara.productivity",
			"icon": "build/icon.png"
		},
		"extraResources": [
			{
				"from": "node_modules/electron-sfsymbols/lib/bin/sfsymbol",
				"to": "bin/sfsymbol",
				"filter": [
					"./resources/**"
				]
			}
		]
	},
